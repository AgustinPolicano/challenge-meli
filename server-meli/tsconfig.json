{
  "compilerOptions": {
    "target": "ESNext", // o "ES6"
    "module": "commonjs",
    "jsx": "react-jsx", // Asegúrate de que esté configurado como "react-jsx"
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
